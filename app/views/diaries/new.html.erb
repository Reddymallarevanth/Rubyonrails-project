<h1>Upload a Diary</h1>
<%= bootstrap_form_with model: @diary, url: diaries_path, method: :post, local: true do |f| %>
    
  <%= f.text_field :name %>
  <%= f.file_field :figure_image %>
  <%= f.text_area :description %>
  <%= f.number_field :price %>
  <%= f.datetime_select :deadline %>
    <%= f.submit %>
<% end %>